    "axios": "^0.20.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fluent-ffmpeg": "^2.1.2",
    "helmet": "^4.1.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.28.0",
    "mongoose": "^5.4.20",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nodeml": "^0.4.0",
    "passport": "^0.4.1",
    "react-redux": "^5.0.7",
    "redux": "^4.0.5",
    "saslprep": "^1.0.3",
    "socket.io": "^2.2.0",
